{
  "name": "stealjs",
  "version": "0.0.1",
  "description": "StealJS's website",
  "main": "index.js",
  "scripts": {
    "document": "bit-docs",
    "document:force": "bit-docs -fd",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stealjs/stealjs.git"
  },
  "keywords": [
    "StealJS"
  ],
  "author": "Bitovi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stealjs/stealjs/issues"
  },
  "homepage": "https://github.com/stealjs/stealjs#readme",
  "dependencies": {
    "grunt-steal": "^1.0.0",
    "steal": "^1.5.18",
    "steal-bundle-manifest": "^1.0.4",
    "steal-conditional": "^0.4.0",
    "steal-css": "^1.2.3",
    "steal-less": "^1.2.0",
    "steal-mocha": "^1.0.0",
    "steal-qunit": "^1.0.0",
    "steal-socket.io": "^4.0.0",
    "steal-tools": "^1.9.2"
  },
  "devDependencies": {
    "bit-docs": "0.0.8-0"
  },
  "bit-docs": {
    "dependencies": {
      "bit-docs-glob-finder": "^0.0.5",
      "bit-docs-dev": "^0.0.3",
      "bit-docs-js": "^0.0.6",
      "bit-docs-tag-sourceref": "^0.0.3",
      "bit-docs-tag-package": "^0.0.3",
      "bit-docs-generate-html": "^0.7.1",
      "bit-docs-prettify": "^0.1.0",
      "bit-docs-html-highlight-line": "^0.2.2",
      "bit-docs-html-toc": "^0.4.0",
      "bit-docs-tag-demo": "^0.3.0",
      "bit-docs-stealjs-theme": "^0.8.1"
    },
    "glob": {
      "pattern": "{node_modules,doc}/{steal,grunt-steal,steal-*}/**/*.{js,md}",
      "ignore": [
        "node_modules/steal/test/**/*",
        "node_modules/steal-tools/test/**/*",
        "node_modules/steal-conditional/{demo,test,node_modules}/**/*",
        "node_modules/grunt-steal/node_modules/**/*"
      ]
    },
    "parent": "StealJS",
    "googleAnalyticsTrackingCode": "UA-2302003-20",
    "minifyBuild": true,
    "dest": "./docs"
  }
}
